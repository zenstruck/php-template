{
    "name": "zenstruck/{name}",
    "description": "{description}",
    "homepage": "https://github.com/zenstruck/{name}",
    "type": "library",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.6.0",
        "symfony/phpunit-bridge": "^6.1|^7.0",
        "symfony/var-dumper": "^5.4|^6.0|^7.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zenstruck\\{namespace}\\": ["src/"] }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\{namespace}\\Tests\\": ["tests/"] }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
